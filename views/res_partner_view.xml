<odoo>
    <record id="view_partner_form_semarnat" model="ir.ui.view">
        <field name="name">res.partner.form.semarnat</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='misc']" position="inside">
                <field name="semarnat_permit"/>
                <field name="semarnat_expiration"/>
            </xpath>
        </field>
    </record>
</odoo>
